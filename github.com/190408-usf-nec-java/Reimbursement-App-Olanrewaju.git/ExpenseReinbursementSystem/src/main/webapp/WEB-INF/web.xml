<!DOCTYPE web-app PUBLIC
"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
"http://java.sun.com/dtd/web-app_2_3.dtd" >

 <web-app>
 <display-name>Archetype Created Web Application</display-name>

 <!-- Servlets  -->
 <servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
 <servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

<servlet>
		<servlet-name>enterReimbursement</servlet-name>
		<servlet-class>com.revature.servlets.EnterReimbServlet</servlet-class>
	</servlet>
 <servlet-mapping>
		<servlet-name>enterReimbursement</servlet-name>
		<url-pattern>/enterReimbursement</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>managerLogin</servlet-name>
		<servlet-class>com.revature.servlets.ManagerLogin</servlet-class>
	</servlet>
 <servlet-mapping>
		<servlet-name>managerLogin</servlet-name>
		<url-pattern>/managerLogin</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>resolve</servlet-name>
		<servlet-class>com.revature.servlets.Resolve</servlet-class>
	</servlet>
 <servlet-mapping>
		<servlet-name>resolve</servlet-name>
		<url-pattern>/resolve</url-pattern>
	</servlet-mapping>

</web-app>
